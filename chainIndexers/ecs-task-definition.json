{
  "family": "moonbeans-indexer-tasks",
  "networkMode": "bridge",
  "containerDefinitions": [
      {
          "name": "moonriver-indexer",
          "image": "748459872289.dkr.ecr.us-east-1.amazonaws.com/moonriver:latest",
          "cpu": 512,
          "memory": 1024,
          "essential": true,
          "portMappings": [
            {
              "containerPort": 8080,
              "hostPort": 8080,
              "protocol": "tcp"
            }
          ],
          "logConfiguration": {
            "logDriver": "awslogs",
            "options": {
              "awslogs-group": "/ecs/moonriver-task-definition",
              "awslogs-region": "us-east-1",
              "awslogs-stream-prefix": "ecs"
            }
          }
      },
      {
        "name": "moonbeam-indexer",
        "image": "748459872289.dkr.ecr.us-east-1.amazonaws.com/moonbeam:latest",
        "cpu": 512,
        "memory": 1024,
        "essential": true,
        "portMappings": [
          {
            "containerPort": 8080,
            "hostPort": 8080,
            "protocol": "tcp"
          }
        ],
        "logConfiguration": {
          "logDriver": "awslogs",
          "options": {
            "awslogs-group": "/ecs/moonbeam-task-definition",
            "awslogs-region": "us-east-1",
            "awslogs-stream-prefix": "ecs"
          }
        }
    }
  ]
}