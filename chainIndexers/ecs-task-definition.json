{
  "family": "moonbeans-indexer-tasks",
  "networkMode": "bridge",
  "containerDefinitions": [
    {
      "name": "moonriver-indexer",
      "image": "748459872289.dkr.ecr.us-east-1.amazonaws.com/moonriver:latest",
      "cpu": 256,
      "memory": 512,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 8081,
          "hostPort": 8081,
          "protocol": "tcp"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/moonriver-task-definition",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    },
    {
      "name": "moonbeam-indexer",
      "image": "748459872289.dkr.ecr.us-east-1.amazonaws.com/moonbeam:latest",
      "cpu": 256,
      "memory": 512,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 8082,
          "hostPort": 8082,
          "protocol": "tcp"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/moonbeam-task-definition",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    },
    {
      "name": "moonriver-holder",
      "image": "748459872289.dkr.ecr.us-east-1.amazonaws.com/moonriver-holder:latest",
      "cpu": 256,
      "memory": 512,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 8083,
          "hostPort": 8083,
          "protocol": "tcp"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/moonriver-holder-task-definition",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    },
    {
      "name": "moonbeam-holder",
      "image": "748459872289.dkr.ecr.us-east-1.amazonaws.com/moonbeam-holder:latest",
