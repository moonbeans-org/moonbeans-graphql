{
  "family": "moonbeans-indexer-tasks",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::748459872289:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "moonriver-indexer",
      "image": "748459872289.dkr.ecr.us-east-1.amazonaws.com/moonriver:latest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 8081,
          "protocol": "tcp"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/moonriver-indexer-serverless",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    },
    {
      "name": "moonbeam-indexer",
      "image": "748459872289.dkr.ecr.us-east-1.amazonaws.com/moonbeam:latest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 8082,
          "protocol": "tcp"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/moonbeam-indexer-serverless",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ]
}